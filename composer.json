{
	"name": "automattic/jetpack-jitm",
	"description": "Just in time messages for Jetpack",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {
		"automattic/jetpack-assets": "1.11.0",
		"automattic/jetpack-connection": "1.22.0",
		"automattic/jetpack-logo": "1.5.0",
		"automattic/jetpack-constants": "1.6.0",
		"automattic/jetpack-options": "1.10.0",
		"automattic/jetpack-partner": "1.4.0",
		"automattic/jetpack-tracking": "1.12.0",
		"automattic/jetpack-redirect": "1.5.0",
		"automattic/jetpack-status": "1.7.0"
	},
	"require-dev": {
		"brain/monkey": "2.6.0",
		"yoast/phpunit-polyfills": "0.2.0"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"repositories": [
		{
			"type": "path",
			"url": "../*"
		}
	],
	"scripts": {
		"phpunit": [
			"@composer install",
			"./vendor/phpunit/phpunit/phpunit --colors=always"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
